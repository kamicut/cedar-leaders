{
  "name": "cedar-leaders",
  "version": "1.0.0",
  "description": "Website for the cedar leaders project",
  "main": "index.js",
  "scripts": {
    "start": "npm run copy && budo src/app.js --open --dir=dist --live --pushstate -- ",
    "copy": "mkdir -p dist && cp src/index.html dist/ && cp -r src/assets dist/assets",
    "build": "npm run copy && browserify src/app.js > dist/app.js"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kamicut/cedar-leaders.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/kamicut/cedar-leaders/issues"
  },
  "homepage": "https://github.com/kamicut/cedar-leaders#readme",
  "dependencies": {
    "choo": "^3.3.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "budo": "^9.2.2"
  }
}
